{
  "expo": {
    "name": "To DO",
    "slug": "todo-app",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash1.png",
      "resizeMode": "cover",
      "backgroundColor": "#F23657"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android":{
      "package":"giho.todo"
    },
    "sdkVersion": "37.0.0",
    "packagerOpts": {
      "nonPersistent": false
    }
  }
}
